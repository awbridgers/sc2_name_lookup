{
  "expo": {
    "name": "SC2 Stats",
    "slug": "sc2-stats",
    "jsEngine": "jsc",
    "version": "1.0.0",
    "orientation": "portrait",
    "primaryColor": "#000000",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#000000"
      },
      "package": "com.awbridgers.sc2stats"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "0e6e7839-b924-44be-8f7b-f6dde819d453"
      }
    },
    "plugins": [
      [
        "./kotlin-version-plugin",
        "1.6.0"
      ]
    ]
  }
}
